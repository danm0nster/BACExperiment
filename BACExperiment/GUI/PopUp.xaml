<Window x:Class="BACExperiment.GUI.PopUp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BACExperiment.GUI"
        mc:Ignorable="d"
        Title="PopUp" Height="200" Width="500" Closed="PopUp_OnClosed">
    <Grid Margin="0,0,0,2">
        <StackPanel>
            <Label x:Name="MessageLabel" Height="117" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></Label>
            <Button Content="OK" Margin="10" Height="25" Width="75"></Button>
        </StackPanel>

    </Grid>
</Window>
