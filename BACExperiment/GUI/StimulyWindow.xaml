<Window x:Class="BACExperiment.StimulyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BACExperiment"
        mc:Ignorable="d"
        Title="StimulyWindow" Height="1000" Width="1000">
    <Grid>
        <Canvas x:Name="StimulyCanvas" RenderTransformOrigin="1,1">
            <Ellipse x:Name="StimulyEllipse1" Width="100" Height="100" Canvas.Top="00" Canvas.Left="00" Stroke="Salmon" StrokeThickness="3"  ></Ellipse>
            <Ellipse x:Name="StimulyEllipse2" Width="100" Height="100" Canvas.Top="100" Canvas.Left="200" Stroke="BurlyWood" StrokeThickness="3"></Ellipse>
        </Canvas>
        </Grid>
</Window>
